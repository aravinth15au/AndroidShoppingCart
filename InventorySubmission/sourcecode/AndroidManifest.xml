<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samsung.model"
    android:versionCode="1"
    android:versionName="1.0" >

    <application
        android:name=".Cart"
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.samsung.activity.InventoryListActivity"
            android:label="@string/inventory" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.samsung.activity.CartListActivity"
            android:label="@string/cart_title" />
        <activity
            android:name="com.samsung.activity.OrderListActivity"
            android:label="@string/order" />
        <activity
            android:name="com.samsung.activity.TransactionListActivity"
            android:label="@string/order" />
    </application>

</manifest>